<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<description></description>
			<from-outcome>home</from-outcome>
			<to-view-id>/front.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>failure</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>error</from-outcome>
			<to-view-id>/error.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>editBean</from-outcome>
			<to-view-id>/editBean.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>editMap</from-outcome>
			<to-view-id>/editMap.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>editor</from-outcome>
			<to-view-id>/editor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>doImage</from-outcome>
			<to-view-id>/image.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>doVideo</from-outcome>
			<to-view-id>/video.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>doFile</from-outcome>
			<to-view-id>/file.xhtml</to-view-id>
		</navigation-case>		
		<navigation-case>
			<description></description>
			<from-outcome>deleted</from-outcome>
			<to-view-id>/front.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<description></description>
			<from-outcome>success</from-outcome>
			<to-view-id>/front.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>retry</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<description></description>
			<from-outcome>does_not_exist</from-outcome>
			<to-view-id>/registerUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/registerUser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>retry</from-outcome>
			<to-view-id>/registerUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/waitingRegistrationConfirmation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/editBean.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/editBean.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleted</from-outcome>
			<to-view-id>/listBeans.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/editBean.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>

	<navigation-rule>
		<from-view-id>/listBeans.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editBean</from-outcome>
			<to-view-id>/editBean.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/listBeans.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/front.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/front.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
