<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	<managed-bean>
		<description>
			Managed bean that is used as an application scope cache
		</description>
		<managed-bean-name>applicationBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.ApplicationBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>productsPerPage</property-name>
			<value>6</value>
		</managed-property>
		<managed-property>
			<property-name>productImageDirUri</property-name>
			<value>..\webapps\theatre\temp</value>
		</managed-property>
		<managed-property>
			<property-name>fileUploadResultPage</property-name>
			<value>uploadImageResult.jsf</value>
		</managed-property>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Service locator of the business services
		</description>
		<managed-bean-name>serviceLocatorBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.ServiceLocatorBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>objectContainerType</property-name>
			<value>db4oClient</value>
		</managed-property>
		<managed-property>
			<property-name>propertiesServerMapWrapper</property-name>
			<value>#{propertiesServerMapWrapper}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
			propertiesServerMapWrapper
		</managed-bean-name>
		<managed-bean-class>ru.scriptum.model.properties.PropertiesServerMapWrapper</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navigationBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.TemplateNavigationBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>navigationMap</property-name>
			<map-entries>
				<map-entry>
					<key>textTemplate</key>
					<value>editor</value>
				</map-entry>
				<map-entry>
					<key>nodeTemplate</key>
					<value>editBean</value>
				</map-entry>
				<map-entry>
					<key>geoTemplate</key>
					<value>editBean</value>
				</map-entry>
				<map-entry>
					<key>inventoryTemplate</key>
					<value>editBean</value>
				</map-entry>
				<map-entry>
					<key>projectTemplate</key>
					<value>editBean</value>
				</map-entry>
				<map-entry>
					<key>imageTemplate</key>
					<value>doImage</value>
				</map-entry>
				<map-entry>
					<key>videoTemplate</key>
					<value>doVideo</value>
				</map-entry>
				<map-entry>
					<key>fileTemplate</key>
					<value>doFile</value>
				</map-entry>
				<map-entry>
					<key>rootTemplate</key>
					<value />
				</map-entry>
				<map-entry>
					<key>mapContainerTemplate</key>
					<value>editBean</value>
				</map-entry>
				<map-entry>
					<key>mapTemplate</key>
					<value>editMap</value>
				</map-entry>
			</map-entries>
		</managed-property>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Managed bean that is used as a session scope cache
		</description>
		<managed-bean-name>sessionBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.SessionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>textBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.TextBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
				<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Extended templated bean with data binding
		</description>
		<managed-bean-name>heteroBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.HeteroBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<!-- managed-property>
			<property-name>templateType</property-name>
			<property-class>java.lang.String</property-class>
			<value>#{param.templateType}</value>
			</managed-property>
			<managed-property>
			<property-name>id</property-name>
			<property-class>java.lang.Long</property-class>
			<value>#{param.id}</value>
			</managed-property -->
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Bean to service the scriptum project structure
		</description>
		<managed-bean-name>projectHandler</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.scriptum.ProjectHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Bean to service the scriptum project lists
		</description>
		<managed-bean-name>projectListHandler</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.scriptum.ProjectListHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Backing bean that contains image information.
		</description>
		<managed-bean-name>uploadBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.UploadBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>id</property-name>
			<value>#{param.id}</value>
		</managed-property>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>Map Design handler</description>
		<managed-bean-name>editMapHandler</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.EditMapHandler</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mapEventBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.dnd.EventBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>editMapHandler</property-name>
			<property-class>ru.scriptum.view.handler.EditMapHandler</property-class>
			<value>#{editMapHandler}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Backing bean that contains a list of Heterobeans.
		</description>
		<managed-bean-name>listBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.ListHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>Template Bean List</description>
		<managed-bean-name>templateListBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.TemplateListHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
		<managed-property>
			<property-name>templateType</property-name>
			<value>#{param.templateType}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<description>
			Backing bean that contains message information.
		</description>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.MessageBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>treeBean</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.TreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>graphHandler</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.GraphHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>serviceLocator</property-name>
			<value>#{serviceLocatorBean}</value>
		</managed-property>
		<managed-property>
			<property-name>id</property-name>
			<value>#{param.id}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginManager</managed-bean-name>
		<managed-bean-class>ru.scriptum.view.handler.LoginManagerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>previousViewId</property-name>
			<value />
		</managed-property>
		<managed-property>
			<property-name>previousAction</property-name>
			<value />
		</managed-property>
		<managed-property>
			<property-name>previousMethod</property-name>
			<value />
		</managed-property>
	</managed-bean>
</faces-config>
